                android:src="@drawable/shape_history_wrapper" />

            <include layout="@layout/history" android:id="@+id/history_10"/>
                        <ImageView
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:src="@drawable/shape_history_wrapper" />

            <include layout="@layout/history" android:id="@+id/history_11"/>

            <ImageView
