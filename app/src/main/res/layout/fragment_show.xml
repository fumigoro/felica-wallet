                android:src="@drawable/shape_history_wrapper" />

            <include layout="@layout/history" android:id="@+id/history_18"/>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:src="@drawable/shape_history_wrapper" />

            <include layout="@layout/history" android:id="@+id/history_19"/>

            <ImageView
                android:layout_width="match_parent"
                android:layout_height="10dp"
                android:src="@drawable/shape_history_wrapper" />

            <include layout="@layout/history" android:id="@+id/history_20"/>

        </LinearLayout>


    </ScrollView>


</androidx.constraintlayout.widget.ConstraintLayout>